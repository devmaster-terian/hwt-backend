                            "    var hwtPedidoVenta = jsonData.hwtPedidoVenta;",
                            "    if(parseInt(hwtPedidoVenta.situacion_pedido) >= 2){",
                            "        elf.hideElement('btnGuardarPedidoVenta');",
                            "        elf.hideElement('btnConfirmarPedidoVenta');",
                            "    }",
                            "",
